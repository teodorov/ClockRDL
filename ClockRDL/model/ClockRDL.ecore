              eType="#/0/model/declarations/Repository" eOpposite="#/0/model/declarations/Repository/items"/>
        </eClassifiers>
        <eClassifiers xsi:type="ecore:EClass" name="Repository" eSuperTypes="#/0/model/kernel/NamedDeclaration">
          <eStructuralFeatures xsi:type="ecore:EReference" name="items" upperBound="-1"
