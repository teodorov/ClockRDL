          <eClassifiers xsi:type="ecore:EClass" name="BooleanLiteral" eSuperTypes="#/0/model/expressions/Literal">
            <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" eType="#/1/Boolean"/>
