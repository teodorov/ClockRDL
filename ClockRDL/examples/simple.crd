import "file:///Users/ciprian/Playfield/repositories/plugmc-java/ClockSystem/ClockRDL/examples/ccsl-kernel.crd"
library simple {

    relation alternates
        clock a b;
    {
        clock c := internal clock[delay]; // c is an internal clock due to the delay expression

        kernel.precedence (isStrict: true a: a b: b)
        kernel.delay (n: 1 base: a delayed: c)
        kernel.precedence (isStrict: true a: b b: c)
    }
}
simple.alternates(
    a: clock[x]
    b:clock[y]
)